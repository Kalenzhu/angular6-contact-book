{
  "name": "Contact",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "number",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "country": {
      "type": "string",
      "default": "India"
    },
    "countryCode": {
      "type": "string",
      "default": "+91"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
